<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<!-- 配置映射 -->
<hibernate-mapping package="entity">
	<class name="MethodPoint" table="methodinfo">
		<id name="mname" column="mname">
         <generator class="assigned">
         </generator>
		</id>
		<property name="mgpsx" column="mgpsx"></property>
		<property name="mgpsy" column="mgpsy"></property>
		<property name="predictprice" column="predictprice"></property>	
		<property name="clusterid" column="clusterid"></property>	
	</class>
</hibernate-mapping>